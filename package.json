{
  "name": "graphql-antd",
  "version": "0.1.0",
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@types/graphql": "^14.0.4",
    "@types/jest": "23.3.10",
    "@types/node": "10.12.12",
    "@types/react": "^16.4.7",
    "@types/react-dom": "16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "antd": "^3.12.1",
    "apollo-cache-inmemory": "^1.3.12",
    "apollo-client": "^2.4.8",
    "apollo-link": "^1.2.6",
    "apollo-link-error": "^1.1.5",
    "apollo-link-http": "^1.5.9",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "http-proxy-middleware": "^0.19.1",
    "moment": "^2.23.0",
    "react": "^16.6.3",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "typescript": "3.2.1"
  },
  "scripts": {
    "start": "yarn start:electron",
    "start:web": "env-cmd .env.web craco start --config craco.config.web.js",
    "start:electron": "electron .",
    "start:electron-dev": "electron electron-dev.js",
    "build": "yarn build:electron",
    "build:web": "craco build --config craco.config.web.js",
    "build:electron": "env-cmd .env.electron craco build --config craco.config.electron.js",
    "watch:electron": "npm-watch",
    "watch": " yarn start:web && yarn watch:electron",
    "test": "craco test",
    "eject": "react-scripts eject",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "watch": {
    "build:electron": "src/**/*.*"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@craco/craco": "^3.3.1",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.5",
    "env-cmd": "^8.0.2",
    "npm-watch": "^0.5.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpackbar": "^3.1.5"
  }
}
